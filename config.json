{
  "Metrics": [

    {
      "Name": "Root volume space",
      "Type": "freespace",
      "Target": "df -h / | awk 'NR>1{print $5}'",
      "MaxValue": "90%"
    },

    {
      "Name": "HTTP req time",
      "Type": "httpreq",
      "Target": "http://localhost/",
      "MaxValue": "100ms"
    },

    {
      "Name": "No TTY users allowed",
      "Type": "command",
      "Target": "who | awk '$2 ~ /tty/ {print $2}'",
      "Output": ""
    }

  ]

}
